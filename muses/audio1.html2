				$('generated-working').innerHTML='';

				$('generated-code').innerHTML='';			

				$('generated').style.display='none';

				$('ffmp3-last-event').innerHTML='';			

			}

			

			function validateForm(){

				clearMessages();

				var ret=true;

				var url=text('url');

				if(url==''){

					addMessage('error','Por favor insira o  <strong>"Numero da Porta"</strong> do Stream.');

					ret=false;

				}

				if(url.substr(url.length-4)=='.m3u'){

					addMessage('error','Please remove the <strong>.m3u</strong> extension from your url. Maybe you\'re trying to use <strong>'+url.substr(0,url.length-4)+'</strong>');

					ret=false;

				}				

				if(text('title')==''){

					addMessage('error','Por favor  preencha o <strong>"Nome da Rádio"</strong>.');

					ret=false;

				}



				if(isNaN(text('volume')) || text('volume') > 100 || text('volume') < 0 ) {

					addMessage('error',' <strong>Volume</strong> inválido. Utilize <strong>números entre 0 e 100</strong>. Ex: <strong>65</strong>');

					ret=false;

				}



				if(text('codec')=='mp3'){

					if(isNaN(text('buffering')) || text('buffering') > 3600 || text('buffering') < 0 ) {

						addMessage('error','<strong>Tempo de Buffering</strong> Inválido. Utilize valores <strong>entre 0 e 3600</strong>. Example: <strong>10</strong>');

						ret=false;

					}

				}



				if(text('wmode')!='window'){

					addMessage('warn','Ao utilizar <strong>WMODE</strong> <strong>"Transparent"</strong> e <strong>"Opaque"</strong> será  <strong>desabilitado o Mouse-Weel-Volume-Control</strong> nos navegadores FFox, Chrome e Safari.');

				}

				if(text('codec')=='ogg'){

					addMessage('warn','Please note that using <strong>OGG</strong> codec will cause FFMp3 to need a well configured <strong>crossdomain.xml</strong> file placed <strong>at the Streaming Server</strong>, otherwise you\'ll get a <strong>"Security Error"</strong> message. Check to the <a href="faq.php">F.A.Q.</a> for details.');

				}

				if(text('wmode')!='transparent'){

					addMessage('warn','Utilizando <strong>WMODE</strong> <strong>"Opaque"</strong> ou <strong>"Window"</strong> requer que você <strong>informe corretamente a cor de fundo de seu site</strong> para evitar que ocorram pixeis brancos em volta das imagens de alguns skins.');

				}				

				if(text('wmode')!='transparent' && !validateColor('#'+text('bgcolor'))){

					addMessage('warn','<strong>Cor de Fundo</strong> Inválida. Utilize o formato <strong>#RRGGBB</strong> . Ex: <strong>#FFFFFF</strong>');

				}

				if($('jsevents').checked){

					addMessage('warn','Please note that using <strong>JS Events</strong> will cause FFMp3 to call a Javascript function named <strong>ffmp3Callback</strong> to inform new events. Check to the <a href="faq.php">F.A.Q.</a> for details.');

				}





				if(skinsArray[text('skin')].swfname=='ffmp3-config.swf'){

					sname=skinsArray[text('skin')].name;

					slink='skins/'+skinsArray[text('skin')].fname+'.zip';

					author=skinsArray[text('skin')].author;

					addMessage('ok','Diferentemente do código gerado diretamente do site do FFMP3 você não precisa salvar nenhum arquivo em seu site utilizando este código.</a>');

				}				

				

				return ret;

			}

			

			function updateFormFields(){

				if(text('wmode')!='transparent'){

					$('bgcolorDIV1').style.display='block';

					$('bgcolorDIV2').style.display='block';

				}else{

					$('bgcolorDIV1').style.display='none';

					$('bgcolorDIV2').style.display='none';

				}			

				if(text('codec')=='mp3'){

					$('bufferingDIV1').style.display='block';

					$('bufferingDIV2').style.display='block';

				}else{

					$('bufferingDIV1').style.display='none';

					$('bufferingDIV2').style.display='none';

				}			

			}

			

			function showIntroInput(){

				if(document.getElementById('introfile').checked){

					document.getElementById('introfileDIV1').style.display='block';

				}else{

					document.getElementById('introfileDIV1').style.display='none';

				}

			}

			

			function validateColor(colorcode){

			     var regColorcode = /^(#)?([0-9a-fA-F]{6})?$/;

			     return regColorcode.test(colorcode);

			}			

			

